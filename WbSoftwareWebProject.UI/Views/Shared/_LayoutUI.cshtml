@using WbSoftwareWebProject.BusinessLayer;
@using WbSoftwareWebProject.Entities.Entity
@{
    ContactManager contactManager = new ContactManager();
    Contact contact = contactManager.Find(x => x.Id == 1);
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>Welat BARAN Kişisel Web Sayfası</title>
    <meta content="Welat BARAN Kişisel Web Sayfası" name="description">
    <meta content="AspNet Mvc5,Javascript,Ajax,Jquery,Mssql,Tsql,EntityFramework,LinQ,Bootstrap5" name="keywords">

    <!-- Favicons -->
    <link href="~/Content/images/logo.png" rel="icon">
    <link href="~/Content/Personal/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!--Font Awesome-->
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />

    <!-- Vendor CSS Files -->
    <link href="~/Content/Personal/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/Personal/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/Content/Personal/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/Content/Personal/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/Content/Personal/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/Content/Personal/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/Content/Personal/assets/css/style.css" rel="stylesheet">
    <link href="~/Content/Site.css" rel="stylesheet" />

</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header">
        <div class="container">

            @*<a href="/Home/Index"><img src="~/Content/images/user_1.jpeg" alt="Welat BARAN" class="img-circle img-responsive" width="200" height="200"></a>*@
            <h1 style="font-size:50px"><a href="/Home/Index">Welat BARAN</a></h1>
            <!-- Uncomment below if you prefer to use an image logo -->
            <h2 style="font-size:30px"><span>Bilgisayar mühendisiyim</span> ve yazılım alanında projeler geliştirmekteyim.</h2>
            <nav id="navbar" class="navbar">
                <ul>
                    <li><a class="nav-link active" href="#header" style="font-size:20px">Anasayfa</a></li>
                    <li><a class="nav-link" href="#about" style="font-size:20px">Hakkımızda</a></li>
                    <li><a class="nav-link" href="#resume" style="font-size:20px">Özgeçmiş</a></li>
                    <li><a class="nav-link" href="#services" style="font-size:20px">Hizmet Alanları</a></li>
                    <li><a class="nav-link" href="#portfolio" style="font-size:20px">Yapılan Projeler</a></li>
                    <li><a class="nav-link" href="#contact" style="font-size:20px">İletişim</a></li>
                </ul>
                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav><!-- .navbar -->
            <br />
            <!-- To-do List : Social Links-->
            <div class="row">
                <a href="@contact.Twitter" target="_blank" class="social"><i class="fa fa-twitter"></i></a>
                <a href="@contact.Instagram" target="_blank" class="social"><i class="fa fa-instagram"></i></a>
                <a href="@contact.Telegram" target="_blank" class="social"><i class="fa fa-telegram"></i></a>
                <a href="@contact.Github" target="_blank" class="social"><i class="fa fa-github"></i></a>
            </div>


        </div>
    </header><!-- End Header -->
    <!-- ======= About Section ======= -->

    @RenderBody()

    <div class="credits">
        Bilgisayar Mühendisi - Welat BARAN tarafından yapıldı. Tüm hakları saklıdır. &copy; @DateTime.Now.Year
    </div>

    <!-- Vendor JS Files -->
    <script src="~/Scripts/jquery-3.0.0.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Content/Personal/assets/vendor/purecounter/purecounter.js"></script>
    <script src="~/Content/Personal/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/Personal/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/Content/Personal/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/Content/Personal/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/Content/Personal/assets/vendor/waypoints/noframework.waypoints.js"></script>
    <script src="~/Content/Personal/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/Content/Personal/assets/js/main.js"></script>

</body>

</html>